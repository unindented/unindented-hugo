{
  "parserOptions": {
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "globals": {
    "p5": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_",
        "caughtErrorsIgnorePattern": "^_",
        "destructuredArrayIgnorePattern": "^_"
      }
    ]
  }
}
